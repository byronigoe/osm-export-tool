## General settings
HOSTNAME=export-tool.example.org
DJANGO_SETTINGS_MODULE=core.settings.project

## SMTP Settings
EMAIL_HOST=<INSECURE_SMTP_HOST> // or smtp.example.com
EMAIL_HOST_USER=<INSECURE_SMTP_USERNAME>
EMAIL_HOST_PASSWORD=<SECURE_SMTP_PASSWORD>
REPLY_TO_EMAIL=<INSECURE_REPLY_EMAIL>

##OAUTH 1 Settings
OSM_API_KEY=<SECURE_OSM_OAUTH1_APP_CONSUMER_KEY>
OSM_API_SECRET=<SECURE_OSM_OAUTH1_APP_CONSUMER_SECRET>

## Workers
WORKER_SECRET_KEY=<SECURE_WORKER_KEY>

## Storage options
EXPORT_DOWNLOAD_ROOT=<DOWNLOAD_ROOT>
EXPORT_STAGING_ROOT=<STAGING_ROOT>

## Deprecated options; Soon to be removed
# GARMIN_MKGMAP=/usr/local/mkgmap/mkgmap.jar
# GARMIN_SPLITTER=/usr/local/splitter/splitter.jar
# GENERATOR_TOOL=/usr/local/bin/generator_tool
# OSMCONVERT=/usr/bin/osmconvert
# OSMAND_MAP_CREATOR_DIR=/usr/local/OsmAndMapCreator

## HDX Options
SYNC_TO_HDX=True
# HDX Options
# Site can be either `demo` or `prod`
HDX_API_KEY=<SECURE_HDX_API_KEY>
HDX_SITE=demo
HDX_NOTIFICATION_EMAIL=<INSECURE_no-reply@example.com>

## Data source API options
OVERPASS_API_URL=https://overpass-example.export.org/api/
RAW_DATA_API_URL=https://raw-data-example_api.export.org/

# Sentry DataSource options
SENTRY_DSN=<SECURE_SENTRY_DSN_URI>
SENTRY_ENVIRONMENT=dev
SENTRY_RELEASE=<INSECURE_RELEASE_STRING>

## Conda specific env variables --- only if you use conda
#PROJ_LIB=/home/youruser/miniconda3/envs/exports/share/proj
#LD_LIBRARY_PATH=/home/youruser/miniconda3/envs/exports/lib/
#GDAL_DATA=/home/youruser/miniconda3/envs/exports/share/gdal